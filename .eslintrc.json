{
  "extends": "next/core-web-vitals",
  "rules": {
    // Customize your ESLint rules here
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "no-console": "warn",
    "react/prop-types": "off"
  },
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  }
}
